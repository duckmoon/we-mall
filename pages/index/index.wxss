/**index.wxss**/
swiper {
  height: 340rpx;
}
swiper image {
  width: 100%;
}
.cates {
  display: flex;
}
.cates-navigator {
  flex: 1;
}
.cates-navigator image {
  padding: 10rpx;
  width: 100%;
}

.floor-title {
  margin: 10rpx 0;
}
.floor-title image {
  width: 100%;
}
.floor-list {
  overflow: hidden;
}
.floor-list navigator {
  width: 33.3%;
  float: left;
}
.floor-list navigator:nth-last-child(-n+4) {
  height: calc(((33.33vw * 386) / 232) / 2 );
  border-left: 10rpx solid #fff;
}
.floor-list navigator:nth-child(2), .floor-list navigator:nth-child(3) {
  border-bottom: 10rpx solid #fff;
}
.floor-list navigator image {
  width: 100%;
  height: 100%;
}